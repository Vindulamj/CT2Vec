#!/usr/bin/python
# -*- coding: utf-8 -*-
import re,string
file = open("results.txt","w") 

with open("output_got.csv","r") as f:
    for line in f:
        y = line.strip().split(";")
	clean_tweet = re.match('(.*?)http.*?\s?(.*?)', y[4])
	if clean_tweet: 
    		file.write(clean_tweet.group(1))
	y =[]	
